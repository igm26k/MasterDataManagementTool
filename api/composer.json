{
  "name": "igm26k",
  "version": "0.1.0",
  "description": "Master Data Management Tool",
  "keywords": [],
  "authors": [
    {
      "name": "Ilya Golubev",
      "email": "golubevim26k@gmail.com",
      "homepage": "http://www.igm26k.ru",
      "role": "Architecturer"
    }
  ],
  "homepage": "",
  "type": "project",
  "license": "Apache-2.0",
  "support": {},
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3.0",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
    "yiisoft/yii2-redis": "~2.0.0",
    "yiisoft/yii2-mongodb": "~2.1.0",
    "mdmsoft/yii2-admin": "~2.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-ldap": "*",
    "ext-redis": "*",
    "ext-mongodb": "*",
    "jenssegers/mongodb": "^3.4",
    "mongofill/mongofill": "dev-master",
    "webonyx/graphql-php": "dev-master",
    "kebacorp/arrayexcelbuilder": "2.0.1",
    "2amigos/yii2-file-upload-widget": "~1.0",
    "kebacorp/vaultsecret": "dev-master",
    "igm26k/yii2-widget-dynamicinputfields": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.1.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/base": "~2.3.0",
    "codeception/verify": "~0.4.0",
    "codeception/specify": "~0.4.6",
    "symfony/browser-kit": ">=2.7 <=4.2.4"
  },
  "config": {
    "vendor-dir": "application/vendor",
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall"
    ],
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "yii\\composer\\Installer::postInstall"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ]
    },
    "yii\\composer\\Installer::postInstall": {
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
